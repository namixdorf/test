<% @cancel_path ||= { :action => :index } %>
<%= render(:partial => '/admin/shared/error_messages_for') -%>
<%= form_for(@form_path.nil? ? [:admin, @object] : @form_path << @object, :html => {:multipart => true, :id => 'object-form', 'data-ajax' => false}) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } -%>
	<button type="submit" class="btn btn-primary">Save</button> 
	<%= link_to('Cancel', @cancel_path, :class =>'btn btn-inverse') -%>
<% end %>